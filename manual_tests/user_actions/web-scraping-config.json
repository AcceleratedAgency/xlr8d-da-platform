{
    "name": "Reviews dumping",
    "scraper": "BrowserStack",
    "type": "dump",
    "url": "https://www.trustpilot.com/review/maxbounty.com",
    "clickPathBefore": [
        "(sleep)",
        "[id*=\"onetrust-accept\"]"
    ],
    "dump": [
        "[class*=\"reviewsContainer\"] article[class*=\"reviewCard\"]"
    ],
    "clickPathAfter": [
        "a[href*=\"/review/\"][name=\"pagination-button-next\"]"
    ],
    "repeat": 2,
    "last_check": null
}